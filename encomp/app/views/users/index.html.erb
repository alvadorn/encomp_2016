<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
            Inscritos
        </h1>
    </div>
</div>
<% if notice %>
<div id="notice">
  <%= notice %>
</div>
<% end %>
<%= form_for user_list_search_path, html: { id: "subscribers_search" }, remote: true do %>
  <p>
    <%= text_field_tag :search, params[:search], placeholder: "Entre com um nome para busca" %>
    <%= submit_tag "Procurar", :name => nil %>
  </p>
  <div id="subscribers"><%= render 'subscribers' %></div>
<% end %>
